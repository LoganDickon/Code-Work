int count = 0;
unsigned long start_time;
int rpm;

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  attachInterrupt(digitalPinToInterrupt(2), counter, RISING);
}

void loop() {
  // put your main code here, to run repeatedly:
  start_time = millis();
  count = 0;
  while((millis() - start_time) < 1000) {
  }
  rpm = count * 60 / 2;
  Serial.print(rpm);
  Serial.println(" rpm");
}

void counter() {
  count++;
}
